<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SELECT - Task Management</title>
    <partial name="_StyleLinks" />
    
</head>
<body>

    <partial name="_Header" />
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <partial name="_Footer" />
    <partial name="_ScriptLinks" />
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
